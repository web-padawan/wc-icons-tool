#!/usr/bin/env node
import { generateIcons } from '../tasks/icons.js';
import { generateLumoFont, generateLumoIconset } from '../tasks/lumo.js';

async function main() {
  await generateIcons();
  await generateLumoIconset();
  await generateLumoFont();
}

main()
  .then(() => {
    console.warn('done');
    process.exit(0);
  })
  .catch((e) => {
    console.error(e);
    process.exit(1);
  });
